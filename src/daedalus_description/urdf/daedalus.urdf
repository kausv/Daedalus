      </collision>
       <visual name="visual">
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
        <geometry>
          <box size="0.25 0.25 0.2"/>
        </geometry>
       </visual>
    </link>
    <link name="leg1link0">
      <inertial>
        <origin xyz="0.892683838 1.03151079 0.148531107" rpy="0 0 0"/>
          <mass value="4.72639954"/>
