      </inertial>
      <collision name="collision">
        <origin xyz="-0.86 -1.03 -0.17" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://daedalus_description/meshes/link1.dae"/>
        </geometry>
      </collision>
      <visual name="visual">
        <origin xyz="-0.86 -1.03 -0.17" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://daedalus_description/meshes/link1.dae"/>
        </geometry>
      </visual>
    </link> 
    <link name="leg1link2">
